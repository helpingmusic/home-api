{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "forceExit": true,
  "modulePaths": [
    "<rootDir>"
  ],
  "moduleDirectories": [
    "../node_modules",
    "../src"
  ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
