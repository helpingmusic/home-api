{
  "name": "home-server",
  "version": "0.0.0",
  "description": "HOME API",
  "author": "Corey Sery",
  "license": "MIT",
  "engines": {
    "node": "8.9.3"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "ts-node -r dotenv/config -r tsconfig-paths/register src/main.ts dotenv_config_path=.env/local.env",
    "build": "rimraf dist; tsc",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/cqrs": "^5.1.1",
    "@nestjs/graphql": "^5.4.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/microservices": "^5.4.0",
    "@nestjs/mongoose": "latest",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/websockets": "^5.4.0",
    "algoliasearch": "^3.30.0",
    "apollo-server-express": "^2.1.0",
    "aws-sdk": "^2.4.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "bull": "^3.4.8",
    "cache-manager": "^2.9.0",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.9.1",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.3",
    "csurf": "^1.9.0",
    "express": "^4.16.4",
    "express-jwt": "^5.0.0",
    "express-session": "^1.11.3",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.2",
    "helmet": "^3.15.0",
    "json2csv": "^4.3.3",
    "jsonwebtoken": "^7.0.0",
    "kue": "^0.11.6",
    "lusca": "^1.3.0",
    "mailchimp-api-v3": "^1.7.0",
    "mandrill-api": "^1.0.45",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "mongoose": "^5.3.13",
    "mongoose-relationship": "^0.1.5",
    "mongoose-validators": "^0.1.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "multiparty": "^4.1.2",
    "nest-bull": "^0.2.4",
    "nest-schedule": "^0.4.3",
    "node-google-calendar": "^1.1.1",
    "node-pre-gyp": "^0.12.0",
    "node-schedule": "^1.3.0",
    "passport": "~0.3.0",
    "passport-facebook": "^2.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.0.1",
    "raven": "^2.4.2",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "s3": "^4.4.0",
    "s3-upload-stream": "^1.0.7",
    "socket.io": "^2.5.0",
    "stripe": "^6.31.2",
    "typescript": "^3.0.1",
    "verror": "^1.10.0",
    "web-push": "^3.2.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/algoliasearch": "^3.30.1",
    "@types/bull": "^3.4.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/mongoose": "^5.2.19",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.10",
    "@types/socket.io": "^2.1.0",
    "@types/stripe": "^6.25.14",
    "@types/supertest": "^2.0.5",
    "dotenv": "^6.1.0",
    "factory-girl": "^5.0.4",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "weak": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^12.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/legacy"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
